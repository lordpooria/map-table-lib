{
  "name": "hesaba",
  "version": "1.0.1",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": "https://gitlab.hesaba.co/visualization-infrastructure/react-dashboard-infra.git",
  "author": "Pooria Rajabzadeh <pooria.rajabzadeh@hesaba.co>",
  "license": "MIT",
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prebuild": "BABEL_ENV=prod  NODE_ENV=production",
    "build": "yarn prebuild -- BABEL_ENV=prod lerna exec --ignore @hesaba/docs -- rollup -c=../../rollup.config.js",
    "build:single": "yarn prebuild -- lerna exec --scope $npm_config_package  -- rollup -c=../../rollup.config.js",
    "dev:single": "lerna exec --scope $npm_config_package --stream -- rollup -c=../../rollup.config.dev.js -w",
    "dev:docs": "lerna run dev --scope @hesaba/docs --stream ",
    "build:docs": "yarn prebuild -- lerna run build --scope @hesaba/docs --stream ",
    "start:docs": "lerna run start --scope @hesaba/docs --stream ",
    "storybook:single": "lerna run storybook --scope $npm_config_package --stream ",
    "dev": "lerna exec -- rollup -c=../../rollup.config.js -w",
    "watch": "lerna run watch",
    "storybook": "start-storybook -p ${PORT:-6006} -h localhost",
    "build-storybook": "build-storybook"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@types/node": "^15.0.2",
    "@types/react": "^17.0.4",
    "@types/react-dom": "^17.0.3",
    "babel-core": "7.0.0-bridge.0",
    "faker": "^5.5.3",
    "find-lerna-packages": "^1.2.0",
    "fs-extra": "^10.0.0",
    "lerna": "^4.0.0",
    "lerna-get-packages": "^1.0.0",
    "recrawl-sync": "^2.2.1",
    "rollup": "^2.39.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-bundle-size": "^1.0.3",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-postcss": "https://github.com/isaacl/rollup-plugin-postcss",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "clsx": "^1.1.1",
    "next": "^9.5.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  }
}
